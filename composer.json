{
    "name": "zonuexe/aliasloader",
    "description": "Class aliasing for provide flat namespace on your application.",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "USAMI Kenta",
            "email": "tadsan@zonu.me"
        }
    ],
    "keywords": [
        "class",
        "loader",
        "alias"
    ],
    "require-dev": {
         "phpunit/phpunit": "^4.8.18"
        ,"theseer/phpdox":  "^0.8.1.1"
        ,"phploc/phploc":   "^2.1.5"
        ,"phpmd/phpmd":     "@stable"
        ,"satooshi/php-coveralls": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Teto\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": ["tests/"]
    },
    "scripts": {
        "test": "./vendor/bin/phpunit"
    }
}
